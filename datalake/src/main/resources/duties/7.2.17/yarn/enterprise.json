{
  "cluster": {
    "blueprintName": "7.2.17 - SDX Medium Duty: Apache Hive Metastore, Apache Ranger, Apache Atlas",
    "validateBlueprint": false
  },
  "customDomain": {
    "domainName": "cloudera.site",
    "hostgroupNameAsHostname": true
  },
  "instanceGroups": [
    {
      "name": "master",
      "template": {
        "yarn": {
          "cpus": 4,
          "memory": 16384
        }
      },
      "nodeCount": 2,
      "type": "CORE",
      "hints": ["az1","az2"]
    },
    {
      "name": "gateway",
      "template": {
        "yarn": {
          "cpus": 4,
          "memory": 16384
        }
      },
      "nodeCount": 2,
      "type": "GATEWAY",
      "hints": ["az2","az3"]
    },
    {
      "name": "core",
      "template": {
        "yarn": {
          "cpus": 4,
          "memory": 16384
        }
      },
      "nodeCount": 3,
      "type": "CORE",
      "hints": ["az1","az2","az3"]
    },
    {
      "name": "auxiliary",
      "template": {
        "yarn": {
          "cpus": 4,
          "memory": 16384
        }
      },
      "nodeCount": 1,
      "type": "CORE",
      "hints": ["az1"]
    },
    {
      "name": "idbroker",
      "template": {
        "yarn": {
          "cpus": 4,
          "memory": 16384
        }
      },
      "nodeCount": 2,
      "type": "CORE",
      "hints": ["az1","az2"]
    }
  ]
}
