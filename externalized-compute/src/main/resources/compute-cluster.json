{
  "api_version": "compute.cloud.cloudera.io/v1alpha1",
  "kind": "ManagedK8sCluster",
  "metadata": {
    "cluster_type": "Dedicated",
    "description": "Common compute cluster",
    "experimental": {
      "deleteClusterOnError": true
    }
  },
  "spec": {
    "deployments": {
      "istio": {
        "deploymentProfile": "cml",
        "enabled": true
      },
      "logging": {
        "enabled": true
      },
      "monitoring": {
        "enabled": true
      }
    },
    "instance_groups": [],
    "security": {
      "apiServer": {
        "enabled": true
      },
      "private": false,
      "secretEncryption": {
        "enabled": false
      }
    }
  }
}
