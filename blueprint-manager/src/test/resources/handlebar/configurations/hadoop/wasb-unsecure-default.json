{

  "core-site": {
    "fs.AbstractFileSystem.wasbs.impl": "org.apache.hadoop.fs.azure.Wasbs",
    "fs.AbstractFileSystem.wasb.impl": "org.apache.hadoop.fs.azure.Wasbs",
    "fs.azure.account.key.accountname.blob.core.windows.net": "accountkey",
    "fs.azure.selfthrottling.read.factor": "1.0",
    "fs.azure.selfthrottling.write.factor": "1.0"
  },
  "core-site": {
    "fs.defaultFS": "wasb://wasb-container@accountname.blob.core.windows.net"
  },
  "hbase-site": {
    "hbase.rootdir": "wasb://wasb-container@accountname.blob.core.windows.net/apps/hbase/data"
  },
  "accumulo-site": {
    "instance.volumes": "wasb://wasb-container@accountname.blob.core.windows.net/apps/accumulo/data"
  },
  "webhcat-site": {
    "templeton.hive.archive": "wasb://wasb-container@accountname.blob.core.windows.net/hdp/apps/${hdp.version}/hive/hive.tar.gz",
    "templeton.pig.archive": "wasb://wasb-container@accountname.blob.core.windows.net/hdp/apps/${hdp.version}/pig/pig.tar.gz",
    "templeton.sqoop.archive": "wasb://wasb-container@accountname.blob.core.windows.net/hdp/apps/${hdp.version}/sqoop/sqoop.tar.gz",
    "templeton.streaming.jar": "wasb://wasb-container@accountname.blob.core.windows.net/hdp/apps/${hdp.version}/mapreduce/hadoop-streaming.jar"
  },
  "oozie-site": {
    "oozie.service.HadoopAccessorService.supported.filesystems": "*"
  }

}