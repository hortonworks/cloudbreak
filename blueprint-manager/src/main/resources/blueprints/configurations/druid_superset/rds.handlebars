{
  "druid-superset-env": {
    "properties": {
      "superset_admin_password": "{{{ cluster_admin_password }}}",
      "superset_admin_firstname": "{{{ cluster_admin_firstname }}}",
      "superset_admin_lastname": "{{{ cluster_admin_lastname }}}",
      "superset_admin_email": "{{{ admin_email }}}"
    }
  }{{{#if rds.superset}}},
  "superset": {
    "properties": {
      "SUPERSET_DATABASE_TYPE" : "{{{ rds.superset.subprotocol }}}",
      "SUPERSET_DATABASE_PASSWORD": "{{{ rds.superset.connectionPassword }}}",
      "SUPERSET_DATABASE_HOSTNAME" : "{{{ rds.superset.host }}}",
      "SUPERSET_DATABASE_NAME": "{{{ rds.superset.databaseName }}}",
      "SUPERSET_DATABASE_PORT" : "{{{ rds.superset.port }}}"
    }
  }{{{/if}}}
}