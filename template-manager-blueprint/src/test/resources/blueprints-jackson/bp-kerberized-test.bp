{
    "Blueprints": {
        "blueprint_name": "multi-node-hdfs-yarn",
        "security": {
            "type": "KERBEROS"
        },
        "stack_name": "HDP",
        "stack_version": "2.5"
    },
    "configurations": [
        {
            "hdfs-site": {
                "properties": {
                    "dfs.replication": 2
                }
            }
        },
        {
            "kerberos-env": {
                "properties": {
                    "admin_server_host": "admin_server_host.bp",
                    "case_insensitive_username_rules": "true",
                    "container_dn": "",
                    "encryption_types": "enc_types.bp",
                    "install_packages": false,
                    "kdc_hosts": "kdc_host.bp",
                    "kdc_type": "mit-kdc",
                    "ldap_url": "",
                    "realm": "REALM.BP"
                }
            }
        },
        {
            "krb5-conf": {
                "properties": {
                    "content": "content.bp",
                    "domains": ".domains.bp",
                    "manage_krb5_conf": "false"
                }
            }
        }
    ],
    "host_groups": [
        {
            "cardinality": "1",
            "components": [
                {
                    "name": "NAMENODE"
                },
                {
                    "name": "SECONDARY_NAMENODE"
                },
                {
                    "name": "RESOURCEMANAGER"
                },
                {
                    "name": "APP_TIMELINE_SERVER"
                },
                {
                    "name": "HISTORYSERVER"
                },
                {
                    "name": "ZOOKEEPER_SERVER"
                },
                {
                    "name": "KERBEROS_CLIENT"
                }
            ],
            "name": "master"
        },
        {
            "cardinality": "2",
            "components": [
                {
                    "name": "DATANODE"
                },
                {
                    "name": "HDFS_CLIENT"
                },
                {
                    "name": "NODEMANAGER"
                },
                {
                    "name": "YARN_CLIENT"
                },
                {
                    "name": "MAPREDUCE2_CLIENT"
                },
                {
                    "name": "ZOOKEEPER_CLIENT"
                },
                {
                    "name": "KERBEROS_CLIENT"
                }
            ],
            "name": "slave_1"
        }
    ]
}