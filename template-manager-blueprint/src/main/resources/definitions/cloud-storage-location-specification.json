      "propertyDisplayName": "Zeppelin Notebooks Root Directory",
      "relatedServices": ["ZEPPELIN_MASTER"],
      "requiredForAttachedCluster": false,
      "supportedStorages": ["ADLS", "WASB", "S3", "GCS", "ADLS_GEN_2"]
    },
    {
