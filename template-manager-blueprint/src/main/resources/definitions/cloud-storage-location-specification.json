      "propertyDisplayName": "Hive Metastore Warehouse External Directory",
      "relatedService": "HIVE_METASTORE",
      "requiredForAttachedCluster": false,
      "supportedStorages": ["ADLS", "WASB", "S3", "GCS", "ADLS_GEN_2"]
    },
    {
