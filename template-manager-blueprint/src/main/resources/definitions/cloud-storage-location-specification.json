      "relatedServices": ["LOGSEARCH_LOGFEEDER"],
      "requiredForAttachedCluster": false,
      "supportedStorages": ["ADLS", "WASB", "S3", "GCS", "ADLS_GEN_2"]
    },
    {
      "propertyName": "zeppelin.notebook.dir",
      "propertyFile": "zeppelin-site",
