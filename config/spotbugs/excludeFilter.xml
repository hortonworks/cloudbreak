<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Bug pattern="DM_DEFAULT_ENCODING" />
    </Match>
    <Match>
        <Bug pattern="DMI_RANDOM_USED_ONLY_ONCE" />
    </Match>
    <Match>
        <Bug pattern="MS_EXPOSE_REP" />
    </Match>
    <Match>
        <Bug pattern="EI_EXPOSE_REP" />
    </Match>
    <Match>
        <Bug pattern="EI_EXPOSE_REP2" />
    </Match>
    <Match>
        <Bug pattern="UC_USELESS_VOID_METHOD" />
    </Match>
    <Match>
        <Bug pattern="RV_RETURN_VALUE_IGNORED_NO_SIDE_EFFECT" />
    </Match>
    <Match>
        <!-- False positive: https://github.com/spotbugs/spotbugs/issues/432 -->
        <Bug pattern="OBL_UNSATISFIED_OBLIGATION" />
    </Match>
    <Match>
        <Bug pattern="DCN_NULLPOINTER_EXCEPTION" />
    </Match>
    <Match>
        <!-- False positive: https://github.com/spotbugs/spotbugs/issues/756 -->
        <Bug pattern="RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE" />
    </Match>
    <Match>
        <Bug pattern="REC_CATCH_EXCEPTION" />
    </Match>
    <Match>
        <Class name="~.*MapperImpl$"/>
    </Match>
    <Match>
        <Class name="~.*EncryptionConverter$"/>
    </Match>
    <Match>
        <Package name="~prometheus.*"/>
    </Match>
    <Match>
        <Class name="~.*Proto$"/>
    </Match>
    <Match>
        <Source name="~.*/generated/.*" />
    </Match>
    <Match>
        <Package name="~org\.apache\.*"/>
    </Match>
    <Match>
        <Package name="~com\.cloudera\.thunderhead\.*"/>
    </Match>
    <Match>
        <Package name="~com\.cloudera\.sigma\.service\.dbus.*"/>
    </Match>
    <Match>
        <Package name="~com\.cloudera\.cdp\.servicediscovery.*"/>
    </Match>
</FindBugsFilter>
