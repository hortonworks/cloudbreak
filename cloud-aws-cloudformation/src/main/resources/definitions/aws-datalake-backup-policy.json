{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DatalakeBackupPermissions",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::<BACKUP_BUCKET>/*"
      ]
    }
  ]
}